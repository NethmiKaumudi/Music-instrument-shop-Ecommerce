// Function to generate unique IDs for each product set
function generateProductSet(product, setId) {
    return product.map((item, index) => ({ ...item, id: setId * 1000 + index + 1 }));
}

// Create 10 sets of products with unique IDs
const productSets = [];
for (let i = 0; i < 10; i++) {
    productSets.push(generateProductSet(product, i));
}

module.exports = productSets;
